        <div class="container">
            <div class="row align-items-center">
                <div class="col-lg-6 col-md-6">
                    <div class="login_part_text text-center">
                    </div>
                </div>
                <div class="col-lg-6 col-md-6">
                    <div class="login_part_form">
                        <div class="login_part_form_iner">
                            <h3>Welcome ! <br>
                                Please Sign up now</h3>
                            <form class="row contact_form" action="#" method="post" novalidate="novalidate">

                                <label>Full Name</label>
                                <div class="col-md-12 form-group p_star">
                                    <input type="text" class="form-control" id="fullname" name="fullname" value=""
                                           placeholder="">
                                </div>

                                <label>Phone</label>
                                <div class="col-md-12 form-group p_star">
                                    <input type="number" class="form-control" id="Phone number" name="Phone" value=""
                                           placeholder="" pattern="[-+]?[0-9]">
                                </div>

                                <label>Email</label>
                                <div class="col-md-12 form-group p_star">
                                    <input type="text" class="form-control" id="Egmail" name="Egmail" value=""
                                           placeholder="">
                                </div>


                                <label>Address</label>
                                <div class="col-md-12 form-group p_star">
                                    <input type="text" class="form-control" id="Address" name="Address" value=""
                                           placeholder="">
                                </div>

                                <label>Birthday</label>
                                <div class="col-md-12 form-group p_star">
                                    <input type="text" class="form-control" id="Birthday" name="Birthday" value=""
                                           placeholder="">
                                </div>


                                <label>User</label>
                                <div class="col-md-12 form-group p_star">
                                    <input type="text" class="form-control" id="name" name="name" value=""
                                           placeholder="" minlength="3" maxlength="12">
                                </div>

                                <label>Password</label>
                                <div class="col-md-12 form-group p_star">
                                    <input type="password" class="form-control" id="password" name="password" value=""
                                           placeholder="" pattern="^(?=.*\d)(?=.*[a-z])(?=.*[A-Z])(?!.*\s).*$"
                                           title="Please include at least 1 uppercase character, 1 lowercase character, and 1 number."
                                           required>
                                </div>

                                <div class="col-md-12 form-group">

                                    <button type="submit" value="submit" class="btn_3">
                                        Sign up
                                    </button>
                                    <a class="lost_pass" href="/login.jsp">Do you have account ? Sign in</a>
                                </div>
                            </form>
                        </div>
                    </div>
